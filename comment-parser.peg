Start
  = Anything* Comment Anything*

Anything
  = .

Comment
  = MultiLineComment
  / SingleLineComment

MultiLineComment
  = "/*" (!"*/" Anything)* "*/"

MultiLineCommentNoLineTerminator
  = "/*" (!("*/" / LineTerminator) Anything)* "*/"

SingleLineComment
  = "//" (!LineTerminator Anything)*

LineTerminator
  = [\n\r\u2028\u2029]
